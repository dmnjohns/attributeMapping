<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://docs.rackspace.com/identity/api/ext/MappingRules"
         version="RAX-1">
    <rules>
        <rule>
            <local>
                <user>
                    <name value="{Pt(apply(concat#3, [&quot;a&quot;, &quot;b&quot;, &quot;c&quot;]))}"/>
                    <email value="{D}"/>
                    <expire value="{D}"/>
                    <domain value="{D}"/>
                    <roles value="{D}"/>
                </user>
            </local>
        </rule>
    </rules>
</mapping>
