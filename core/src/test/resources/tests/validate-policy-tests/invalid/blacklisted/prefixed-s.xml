<?xml version="1.0" encoding="UTF-8"?>
<mapping
    xmlns="http://docs.rackspace.com/identity/api/ext/MappingRules"
    xmlns:foo="http://www.w3.org/2005/xpath-functions"
    version="RAX-1">
    <rules>
        <rule>
            <local>
                <user>
                    <name value="{D}"/>
                    <email value="{D}"/>
                    <expire value="{D}"/>
                    <domain value="{D}"/>
                    <roles value="{Pts(foo:doc('file:///example.xml'))} {Pts(foo:doc-available('file:///example.xml'))}"/>
                </user>
            </local>
        </rule>
    </rules>
</mapping>
