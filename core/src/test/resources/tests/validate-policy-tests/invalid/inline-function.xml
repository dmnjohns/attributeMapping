<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://docs.rackspace.com/identity/api/ext/MappingRules"
         version="RAX-1">
    <rules>
        <rule>
            <local/>
            <remote>
                <attribute path="
                    let $addOne := function($i as xs:int) as xs:int { $i + 1 }
                    return $addOne(1)"/>
            </remote>
        </rule>
    </rules>
</mapping>
