{
    "mapping": {
        "rules": [
            {
                "remote": [
                    {
                        "path": "let $myMap := map { \"foo\": \"bar\" } return map:get($myMap, \"foo\")"
                    }
                ],
                "local": {}
            }
        ],
        "version": "RAX-1",
        "namespaces": {
            "map": "http://www.w3.org/2005/xpath-functions/map"
        }
    }
}
